var province = ee.FeatureCollection("users/mahyouh/Province");


var Haouz = ee.FeatureCollection("users/mahyouh/Province")
.filter(ee.Filter.inList("Nom_Provin", ["Al  Haouz"]));


//Map.addLayer(province)

